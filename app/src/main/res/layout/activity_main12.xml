<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity12"
    android:background="@drawable/background">


    <Button
        android:id="@+id/next_btn3"
        android:layout_width="167dp"
        android:layout_height="48dp"
        android:background="@drawable/btn_shape"
        android:fontFamily="@font/bebas_neue"
        android:text="Back"
        android:textColor="@color/white"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.935" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_left_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_right_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_down_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9507524" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_strt_dtls"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.18" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_end_dtls"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="405dp" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_left_dtls"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.28" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_strt_lst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.57" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_end_lst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.85" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="375dp"
        android:layout_height="124dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/gd_right_05"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/gd_left_05"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/name"
        android:layout_width="92dp"
        android:layout_height="37dp"
        android:fontFamily="@font/bebas_neue"
        android:text="Name :"
        android:textColor="@color/white"
        android:textSize="22sp"
        android:paddingLeft="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/gd_end_dtls"
        app:layout_constraintEnd_toEndOf="@+id/gd_left_dtls"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gd_left_05"
        app:layout_constraintTop_toTopOf="@+id/gd_strt_dtls"
        app:layout_constraintVertical_bias="0.022">

    </TextView>

    <TextView
        android:id="@+id/phone"
        android:layout_width="92dp"
        android:layout_height="37dp"
        android:fontFamily="@font/bebas_neue"
        android:paddingLeft="10sp"
        android:text="Phone No. :"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/gd_end_dtls"
        app:layout_constraintEnd_toEndOf="@+id/gd_left_dtls"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gd_left_05"
        app:layout_constraintTop_toTopOf="@+id/gd_strt_dtls"
        app:layout_constraintVertical_bias="0.258"></TextView>

    <TextView
        android:id="@+id/age"
        android:layout_width="92dp"
        android:layout_height="37dp"
        android:fontFamily="@font/bebas_neue"
        android:paddingLeft="10sp"
        android:text="Age :"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/gd_end_dtls"
        app:layout_constraintEnd_toEndOf="@+id/gd_left_dtls"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gd_left_05"
        app:layout_constraintTop_toTopOf="@+id/gd_strt_dtls"
        app:layout_constraintVertical_bias="0.495"></TextView>

    <TextView
        android:id="@+id/gender"
        android:layout_width="92dp"
        android:layout_height="37dp"
        android:fontFamily="@font/bebas_neue"
        android:text="Weight :"
        android:textColor="@color/white"
        android:textSize="22sp"
        android:paddingLeft="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/gd_end_dtls"
        app:layout_constraintEnd_toEndOf="@+id/gd_left_dtls"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gd_left_05"
        app:layout_constraintTop_toTopOf="@+id/gd_strt_dtls"
        app:layout_constraintVertical_bias="0.950">
    </TextView>

    <TextView
        android:id="@+id/gender2"
        android:layout_width="92dp"
        android:layout_height="37dp"
        android:fontFamily="@font/bebas_neue"
        android:paddingLeft="10sp"
        android:text="Height :"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toEndOf="@+id/gender"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.512"></TextView>

    <TextView
        android:id="@+id/gender3"
        android:layout_width="92dp"
        android:layout_height="37dp"
        android:fontFamily="@font/bebas_neue"
        android:paddingLeft="10sp"
        android:text="Gender :"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/gd_end_dtls"
        app:layout_constraintEnd_toEndOf="@+id/gd_left_dtls"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gd_left_05"
        app:layout_constraintTop_toTopOf="@+id/gd_strt_dtls"
        app:layout_constraintVertical_bias="0.711"></TextView>

    <TextView
        android:id="@+id/name_set"
        android:layout_width="256dp"
        android:layout_height="36dp"
        android:fontFamily="@font/bebas_neue"
        android:paddingLeft="10sp"
        android:text="Name Error"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/gd_end_dtls"
        app:layout_constraintEnd_toEndOf="@+id/gd_right_05"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintStart_toStartOf="@+id/gd_left_dtls"
        app:layout_constraintTop_toTopOf="@+id/gd_strt_dtls"
        app:layout_constraintVertical_bias="0.022">

    </TextView>

    <TextView
        android:id="@+id/phone_set"
        android:layout_width="256dp"
        android:layout_height="36dp"
        android:fontFamily="@font/bebas_neue"
        android:paddingLeft="10sp"
        android:text="Phone No.Error"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/gd_end_dtls"
        app:layout_constraintEnd_toEndOf="@+id/gd_right_05"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="@+id/gd_left_dtls"
        app:layout_constraintTop_toTopOf="@+id/gd_strt_dtls"
        app:layout_constraintVertical_bias="0.257">

    </TextView>

    <TextView
        android:id="@+id/age_set"
        android:layout_width="256dp"
        android:layout_height="36dp"
        android:fontFamily="@font/bebas_neue"
        android:paddingLeft="10sp"
        android:text="Age Error"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/gd_end_dtls"
        app:layout_constraintEnd_toEndOf="@+id/gd_right_05"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="@+id/gd_left_dtls"
        app:layout_constraintTop_toTopOf="@+id/gd_strt_dtls"
        app:layout_constraintVertical_bias="0.497">

    </TextView>

    <TextView
        android:id="@+id/gender_set"
        android:layout_width="256dp"
        android:layout_height="36dp"
        android:fontFamily="@font/bebas_neue"
        android:paddingLeft="10sp"
        android:text="Gender Error"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/gd_end_dtls"
        app:layout_constraintEnd_toEndOf="@+id/gd_right_05"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="@+id/gd_left_dtls"
        app:layout_constraintTop_toTopOf="@+id/gd_strt_dtls"
        app:layout_constraintVertical_bias="0.713">

    </TextView>

    <EditText
        android:id="@+id/desc_text"
        android:layout_width="370dp"
        android:layout_height="133dp"
        android:background="@drawable/edt_background"
        android:ems="0"
        android:fontFamily="@font/bebas_neue"
        android:gravity="top"
        android:hint="Type Here..."
        android:inputType="textLongMessage"
        android:lines="10"
        android:maxLines="50"
        android:padding="10dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/gd_end_lst"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/gd_strt_lst">

    </EditText>

    <TextView
        android:id="@+id/desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bebas_neue"
        android:text="Description :"
        android:textColor="@color/white"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/desc_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/gd_end_dtls"
        app:layout_constraintVertical_bias="0.473">

    </TextView>

</androidx.constraintlayout.widget.ConstraintLayout>